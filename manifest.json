{
    "manifest_version" : 3,
    "name" : "More Roblox accounts",
    "description" : "What you neeed a description!?",
    "version" : "1.0",
    "background" : {
        "service_worker" : "helper.js"
    },
    "permissions": ["activeTab", "scripting", "tabs", "activeTab", "<all_urls>"],
    "host_permissions": [
        "http://*/",
        "https://*/",
        "https://roblox.com/*",
        "https://www.roblox.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://roblox.com/*", "https://www.roblox.com/*"],
            "js": ["content.js"]
        }
    ]
}